      <center><%= image_tag chip.image.url, class: "responsive-img"%></center>
    </div>
    <div class="card-content" id="<%= chip.slug%>">
      <%= render partial: 'admin/chips/chip_layout', locals: {chip: chip, source: "index"} %>
    </div>
  </div>
</div>
