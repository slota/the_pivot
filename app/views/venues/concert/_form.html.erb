<%= form_for [:venue, @concert] do |f| %>
  <%= f.label :date %>
  <%= f.date_field :date %>
  <%= f.label :band %>
  <%= f.text_field :band %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.label :logo %>
  <br>
  <%= f.file_field :logo %>
  <br>
  <br>
  <%= f.submit %>
<% end %>
